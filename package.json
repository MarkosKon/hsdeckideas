{
  "name": "hs-deck-ideas",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build && react-snapshot",
    "test": "react-app-rewired test --env=jsdom",
    "analyze": "node bundle-analyzer-cra.js",
    "spit:dep": "yarn list > analyze/npmlist.txt",
    "netlify": "netlify deploy --dir build",
    "deploy": "yarn build && netlify deploy --dir build --prod"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "reactSnapshot": {
    "include": [
      "/FAQ",
      "/new-features"
    ],
    "snapshotDelay": 300
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-brands-svg-icons": "^5.6.3",
    "@fortawesome/free-regular-svg-icons": "^5.6.3",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "already-styled-components": "^3.0.2",
    "balloon-css": "^0.5.0",
    "compare-object-field": "^1.2.2",
    "d3-axis": "^1.0.12",
    "d3-hierarchy": "^1.1.8",
    "d3-scale": "^2.1.2",
    "d3-selection": "^1.3.2",
    "d3-shape": "^1.2.2",
    "d3-zoom": "^1.7.3",
    "deckstrings": "^1.1.0",
    "immutability-helper": "^2.9.0",
    "lodash.get": "^4.4.2",
    "lodash.partial": "^4.2.1",
    "lodash.sortby": "^4.7.0",
    "memoize-one": "^4.0.3",
    "normalize.css": "^8.0.1",
    "polished": "^2.3.1",
    "react": "^16.7.0",
    "react-app-polyfill": "^0.2.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.7.0",
    "react-ga": "^2.5.6",
    "react-helmet": "^5.2.0",
    "react-lazyload": "^2.3.0",
    "react-loadable": "^5.5.0",
    "react-modal": "^3.8.1",
    "react-progressive-bg-image": "^3.0.0",
    "react-progressive-image": "^0.6.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "react-select": "^1.3.0",
    "react-select-fast-filter-options": "^0.2.3",
    "react-virtualized": "^9.21.0",
    "react-virtualized-select": "^3.1.3",
    "some-utils": "^1.0.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "customize-cra": "^0.2.8",
    "react-app-rewired": "^1.6.2",
    "react-snapshot": "^1.3.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "workerize-loader": "^1.0.4"
  }
}
