{
  "name": "hs-deck-ideas",
  "version": "2.3.0",
  "description": "A random deck generator for Hearthstone.",
  "author": "Markos Konstantopoulos",
  "main": "src/index.jsx",
  "scripts": {
    "start": "webpack-dev-server --hot --config webpack/webpack.dev.js",
    "build": "webpack --config webpack/webpack.prod.js",
    "build:static": "yarn build && react-snap",
    "test": "jest",
    "test:staged": "jest --findRelatedTests",
    "test:watch": "jest --watch",
    "test:coverage": "yarn jest --coverage --collectCoverageFrom=\"src/**/*.{jsx,js}\"",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "lint": "eslint --ext js,jsx src",
    "lint:fix": "eslint --ext js,jsx src --fix",
    "format": "prettier-eslint --write \"src/**/*.{js,jsx}\"",
    "serve": "serve -s dist",
    "deploy": "netlify deploy --dir=dist",
    "deploy:prod": "netlify deploy --prod --dir=dist"
  },
  "reactSnap": {
    "source": "dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "flow check && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint",
      "prettier-eslint --write",
      "git add",
      "yarn test:staged"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "react-testing-library/cleanup-after-each"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-brands-svg-icons": "^5.6.3",
    "@fortawesome/free-regular-svg-icons": "^5.6.3",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "already-styled-components": "^3.0.2",
    "compare-object-field": "^1.2.2",
    "core-js": "2",
    "d3-array": "^2.0.3",
    "d3-axis": "^1.0.12",
    "d3-hierarchy": "^1.1.8",
    "d3-scale": "^2.1.2",
    "d3-selection": "^1.3.2",
    "d3-shape": "^1.2.2",
    "d3-zoom": "^1.7.3",
    "deckstrings": "1.1.0",
    "formik": "^2.0.6",
    "immutability-helper": "^2.9.0",
    "jsondiffpatch": "^0.3.11",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.partial": "^4.2.1",
    "lodash.partition": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "memoize-one": "4.0.3",
    "microtip": "^0.2.2",
    "normalize.css": "^8.0.1",
    "polished": "^2.3.1",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.7.0",
    "react-ga": "^2.5.6",
    "react-helmet": "^5.2.0",
    "react-lazyload": "^2.3.0",
    "react-loadable": "^5.5.0",
    "react-modal": "^3.8.1",
    "react-progressive-image": "^0.6.0",
    "react-router-dom": "^4.3.1",
    "react-select": "1.3.0",
    "react-select-fast-filter-options": "^0.2.3",
    "react-toastify": "^5.4.1",
    "react-virtualized": "^9.21.0",
    "react-virtualized-select": "^3.1.3",
    "some-utils": "^1.0.0",
    "styled-components": "^4.1.3",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-styled-components": "^1.10.0",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "cypress": "^3.2.0",
    "cypress-testing-library": "^2.4.0",
    "dotenv": "^6.2.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^5.11.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-flowtype": "^3.6.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^1.0.1",
    "file-loader": "^3.0.1",
    "flow-bin": "^0.97.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "jest-dom": "^3.1.3",
    "lint-staged": "^8.1.5",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "react-snap": "^1.23.0",
    "react-test-renderer": "^16.7.0",
    "react-testing-library": "^6.1.2",
    "regenerator-runtime": "^0.13.1",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "serve": "^11.2.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.28.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.5",
    "workbox-webpack-plugin": "^3.6.3",
    "workerize-loader": "^1.0.4"
  }
}
